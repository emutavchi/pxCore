var loggingLevel=1;function Logger(a){this.name=a}Logger.prototype.getHeading=function(a){return a+" ["+this.name+"]: "},Logger.prototype.fullMessage=function(a,b){return this.getHeading(a)+b},Logger.prototype.error=function(a){console.error(this.fullMessage("ERROR",a))},Logger.prototype.warn=function(a){console.warn(this.fullMessage("WARN",a))},Logger.prototype.info=function(a){this.message(1,a)},Logger.prototype.message=function(a,b){a>loggingLevel||console.log(this.fullMessage("MESSAGE:"+a,b))};function setLoggingLevel(a){loggingLevel=a}module.exports={Logger:Logger,setLoggingLevel:setLoggingLevel};